{
    "name": "spyck/spreadsheet",
    "description": "Simple PHP tool for parsing CSV or Excel spreadsheets",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Verspyck",
            "email": "willemverspyck@outlook.com",
            "homepage": "https://verspyck.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "csv",
        "excel",
        "filter",
        "parse",
        "sdk",
        "spreadsheet",
        "tsv",
        "txt"
    ],
    "homepage": "https://github.com/willemverspyck/spreadsheet",
    "type": "library",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3",
        "friendsofphp/php-cs-fixer": "^3",
        "phpoffice/phpspreadsheet": "^2|^3|^4",
        "phpunit/phpunit": "^12",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Spyck\\Spreadsheet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spyck\\Spreadsheet\\Tests\\": "tests/"
        }
    }
}
